	"autoload": {
		"psr-4": {
			"Kdyby\\Console\\": "src/"
		},
		"classmap": [
			"src/CliPresenter.php",
			"src/exceptions.php"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.7-dev"
		}
	}
}
