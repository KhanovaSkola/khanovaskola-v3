{
	"name": "nette/nette",
	"type": "library",
	"description": "Nette Framework - innovative framework for fast and easy development of secured web applications in PHP. Write less, have cleaner code and your work will bring you joy.",
	"keywords": ["framework", "mvc", "debugging", "templating", "database", "forms", "mailing"],
	"homepage": "http://nette.org",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "David Grudl",
			"homepage": "http://davidgrudl.com"
		},
		{
			"name": "Nette Community",
			"homepage": "http://nette.org/contributors"
		}
	],
	"require": {
		"nette/application": "2.3.0-beta",
		"nette/bootstrap": "2.3.0-RC",
		"nette/caching": "2.3.0-RC",
		"nette/component-model": "2.2.0",
		"nette/database": "2.3.0-RC",
		"nette/deprecated": "2.2.1",
		"nette/di": "2.3.0-beta",
		"nette/finder": "2.3.0-RC",
		"nette/forms": "2.3.0-beta",
		"nette/http": "2.3.0-RC",
		"nette/mail": "2.3.0-RC",
		"nette/neon": "2.3.0-RC",
		"nette/php-generator": "2.3.0-RC",
		"nette/reflection": "2.3.0-RC",
		"nette/robot-loader": "2.3.0-RC",
		"nette/safe-stream": "2.3.0",
		"nette/security": "2.3.0-RC",
		"nette/tokenizer": "2.2.0",
		"nette/utils": "2.3.0-RC",
		"latte/latte": "2.3.0-RC",
		"tracy/tracy": "2.3.0-RC"
	},
	"require-dev": {
		"nette/tester": "~1.3"
	},
	"autoload": {
		"classmap": ["Nette/"]
	},
	"minimum-stability": "dev",
	"extra": {
		"branch-alias": {
			"dev-master": "2.3-dev"
		}
