{
    "name": "nextras/tracy-query-panel",
    "type": "library",
    "description": "Query panel for Tracy php debugger",
    "keywords": ["tracy", "nextras"],
    "homepage": "https://github.com/nextras/tracy-query-panel",
    "license": "MIT",
    "authors": [
        { "name": "Nextras Project", "homepage": "https://github.com/nextras/tracy-query-panel/graphs/contributors" }
    ],
    "support": {
        "issues": "https://github.com/nextras/tracy-query-panel/issues"
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4",
        "latte/latte": "~2.2.0 || ~2.3.0-beta",
        "nette/di": "~2.3.0-beta",
        "tracy/tracy": "~2.3.0-RC"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "nette/nette": "~2.3.0-beta",
        "nette/tester": "~1.3",
        "petrp/access": "~0.2.2"
    },
    "autoload": {
        "psr-4": {
            "Nextras\\TracyQueryPanel\\": "src/TracyQueryPanel",
            "Nextras\\TracyQueryPanel\\Handlers\\": "src/handlers"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
