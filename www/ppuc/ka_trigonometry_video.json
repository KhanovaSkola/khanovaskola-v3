[{"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:31Z", "popis": "Poznáváme sinus, kosinus a tangens a příklady řešení pro zadaný pravoúhlý trojúhelník.", "nazev": "Úvod do goniometrických funkcí", "url": "https://cs.khanacademy.org/video/basic-trigonometry", "id": "19647490", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:31Z", "popis": "Ukážeme si několik příkladů, jak začít se dvěma rameny pravoúhlého trojúhelníku a najít tak goniometrickou funkci jednoho ze dvou ostrých úhlů.", "nazev": "Trigonometrické funkce v pravoúhlém trojúhelníku", "url": "https://cs.khanacademy.org/video/basic-trigonometry-ii", "id": "19647491", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:32:55Z", "popis": "Při zadaném pravoúhlém trojúhelníku s ostrým úhlem 65° a ramenem dlouhým 5 jednotek, použij trigonometrii k nalezení dvou chybějících stran.", "nazev": "Hledání délky strany v pravoúhlém trojúhelníku pomocí trigonometrie", "url": "https://cs.khanacademy.org/video/example-trig-to-solve-the-sides-and-angles-of-a-right-triangle", "id": "1153614407", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:51Z", "popis": "Řešení příkladu na modelování, kde je hledán potřebný úhel k zastřelení vetřelce.", "nazev": "Slovní úlohy s pravoúhlými trojúhelníky", "url": "https://cs.khanacademy.org/video/angle-to-aim-to-get-alien", "id": "x9a39e2a2", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:42Z", "popis": "Zjisti, jak jsou goniometrické funkce odvozeny z pohledu podobnosti trojúhelníků.", "nazev": "Podobnost trojúhelníků a goniometrické funkce", "url": "https://cs.khanacademy.org/video/similarity-to-define-sine-cosine-and-tangent", "id": "xc0627a6a", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:32:55Z", "popis": "Na základě zadaného diagramu s více pravoúhlými trojúhelníky ověř identitu vztahující se ke goniometrickým funkcím v těchto trojúhelnících.", "nazev": "Trigonometrické výzvy: ověření identity", "url": "https://cs.khanacademy.org/video/example-with-trig-functions-and-ratios", "id": "x835650f9", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:20Z", "popis": "Na základě zadaného diagramu s více pravoúhlými trojúhelníky zařiď, aby různé výrazy odpovídaly různým hodnotám goniometrických funkcí.", "nazev": "Trigonometrické výzvy: Hodnoty goniometrických funkcí", "url": "https://cs.khanacademy.org/video/example-relating-trig-function-to-side-ratios", "id": "x98eb8563", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:32Z", "popis": "Ukážeme si, že sinus jakéhokoli úhlu se rovná kosinu jeho doplňkového úhlu.", "nazev": "Sinus a kosinus doplňkových úhlů", "url": "https://cs.khanacademy.org/video/showing-relationship-between-cosine-and-sine-of-complements", "id": "x71f71130", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:57Z", "popis": "Řešíme následující příklad: Víme, že cos(58°)=0,53, najdi sin(32°).", "nazev": "Používání doplňkových úhlů", "url": "https://cs.khanacademy.org/video/sine-and-cosine-of-complements-example", "id": "xee258334", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:55Z", "popis": "Řešíme příklad týkající se podmořské pyramidy za použití skutečnosti, že sinus úhlu je roven kosinu jeho doplňkového úhlu.", "nazev": "Slovní úlohy: doplňkové úhly", "url": "https://cs.khanacademy.org/video/how-much-of-a-pyramid-is-submerged", "id": "xf177c90b", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:27:34Z", "popis": "Zavádění a dokazování goniometrické jedničky (sinθ)^2+(cosθ)^2=1, která vyplývá z Pythagorovy věty!", "nazev": "Úvod do goniometrické jedničky", "url": "https://cs.khanacademy.org/video/pythagorean-trig-identity-from-soh-cah-toa", "id": "x10c52471", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:27:06Z", "popis": "Najdi všech šest goniometrických funkcí (sinus, kosinus, tangens, sekans, kosekans a kotangens) úhlu v daném pravoúhlém trojúhelníku.", "nazev": "Hledání převrácených goniometrických funkcí", "url": "https://cs.khanacademy.org/video/example-the-six-trig-ratios", "id": "1153576581", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:26:35Z", "popis": "Při zadaných dvou stranách pravoúhlého trojúhelníku a kotangensu jednoho z úhlů, najdi chybějící dělku strany.", "nazev": "Používání převrácených goniometrických funkcí", "url": "https://cs.khanacademy.org/video/example-using-trig-to-solve-for-missing-information", "id": "1153530471", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-09-09T06:35:32Z", "popis": "Máme zadaný trojúhelník přičemž známe velikosti dvou jeho úhlů a délku jedné strany. Pomocí sinové věty dopočítáme délku zbývajících dvou stran.", "nazev": "Řešení délky strany se sinovou větou", "url": "https://cs.khanacademy.org/video/law-of-sines", "id": "x29e70bc4", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-09-09T06:35:34Z", "popis": "Máme zadaný trojúhelník přičemž známe délky dvou jeho stran a velikost jednoho úhlu. Pomocí sinové věty dopočítáme velikost zbývajících dvou úhlů.", "nazev": "Řešení velikosti úhlů se sinovou větou", "url": "https://cs.khanacademy.org/video/law-of-sines-example", "id": "x955c68f1", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:43Z", "popis": "Jednoduchý důkaz sinové věty", "nazev": "Dokazování sinové věty", "url": "https://cs.khanacademy.org/video/proof-law-of-sines", "id": "26258", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-09-09T06:35:33Z", "popis": "Zjisti, jak použít kosinovou větu k nalezení chybějící délky stran trojúhleníku se dvěma zadanými délkami stran a velikostí jednoho úhlu.", "nazev": "Hledání délky strany pomocí kosinové věty", "url": "https://cs.khanacademy.org/video/law-of-cosines-example", "id": "x17ddf742", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-09-09T06:35:31Z", "popis": "Máme zadaný trojúhelník. přičemž známe délky všech jeho stran. Naším úkolem je najít velikost jednoho úhlu pomocí kosinové věty.", "nazev": "Hledání velikosti úhlu pomocí kosinové věty", "url": "https://cs.khanacademy.org/video/law-of-cosines-missing-angle", "id": "xb7d3bb05", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:41Z", "popis": "Jednoduchý důkaz kosinové věty.", "nazev": "Dokazování kosinové věty", "url": "https://cs.khanacademy.org/video/law-of-cosines", "id": "26254", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:46Z", "popis": "Pomocí kosinové věty si vypočítáme slovní úlohu týkající se vzdálenosti mezi hvězdami.", "nazev": "Trigonometrické slovní úlohy: hvězdy", "url": "https://cs.khanacademy.org/video/law-of-cosines-word-problem", "id": "x461dc00c", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:47Z", "popis": "Vysvětlení definice a motivace radiánů a vztah mezi radiány a stupni.", "nazev": "Úvod do radiánů", "url": "https://cs.khanacademy.org/video/introduction-to-radians", "id": "1159577071", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:24Z", "popis": "Obecný přístup k převodu mezi radiány a stupni a naopak.", "nazev": "Radiány a stupně", "url": "https://cs.khanacademy.org/video/radian-and-degree-conversion-practice", "id": "1159451550", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:22Z", "popis": "Pojďme si ukázat, jak na převádění stupňů o velikosti 150° a -45° na radiány.", "nazev": "Stupně na radiány", "url": "https://cs.khanacademy.org/video/we-converting-degrees-to-radians", "id": "1153849058", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:06Z", "popis": "Pojďme si ukázat, jak na převádění radiánů o velikosti  radiánů π a -π/3 na stupně.", "nazev": "Radiány na stupně", "url": "https://cs.khanacademy.org/video/we-converting-radians-to-degrees", "id": "1153727649", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:00Z", "popis": "Společně si vysvětlíme určování kvadrantu pro danou hodnotu radiánů.", "nazev": "Radiány úhlů a kvadranty", "url": "https://cs.khanacademy.org/video/rotation-by-radians-and-quadrants", "id": "x81df7889", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:33Z", "popis": "Zjisti, jak použít jednotkovou kružnici k definování sinus, kosinus a tangens pro všechna reálná čísla.", "nazev": "Jednotková kružnice", "url": "https://cs.khanacademy.org/video/unit-circle-definition-of-trig-functions-1", "id": "1159990979", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:15Z", "popis": "Ukážeme si, že při řešení úloh s trojúhelníkem s ostrými úhly můžou vyjít dva správné výsledky.  Využijeme pro to anglického způsobu s pomůckou SOH CAH TOA a definice jednotkové kružnice.", "nazev": "Goniometrické funkce u pravoúhlého trojúhelníku", "url": "https://cs.khanacademy.org/video/matching-ratios-trig-functions", "id": "xdcacd12a", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-12T06:32:00Z", "popis": "Graf funkce y=sin(x) je jako vlna, která pravidelně osciluje mezi -1 a 1 ve vzoru, který se opakuje každé 2π jednotky. Konkrétně to znamená, že definiční obor funkce sin(x) jsou všechna reálná čísla a obor hodnot je [-1,1]. Ukážeme si, jak pomocí jednotkové kružnice odvodit graf funkce y=sin(x).", "nazev": "Graf funkce y=sin(x)", "url": "https://cs.khanacademy.org/video/we-graph-domain-and-range-of-sine-function", "id": "1153456927", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:33Z", "popis": "Na základě jednotkové kružnice si zakreslíme graf funkce tangens.", "nazev": "Graf funkce y=tan(x)", "url": "https://cs.khanacademy.org/video/tangent-graph", "id": "x9267c937", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-08-30T06:35:29Z", "popis": "Nejprve si nakreslíme grafy funkcí sinus a kosinus a poté si ukážeme, kde mají průsečíky.", "nazev": "Průsečíky funkcí y=sin(x) a y=cos(x)", "url": "https://cs.khanacademy.org/video/we-graphs-of-sine-and-cosine-functions", "id": "1153664550", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:49Z", "popis": "Společně si pomocí symetrie jednotkové kružnice odvodíme několik užitečných vzorců pro sinus a kosinus.", "nazev": "Vzorce pro sinus a kosinus na základě symetrie", "url": "https://cs.khanacademy.org/video/trigonometry-unit-circle-symmetry", "id": "xa88b4a7e", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:33Z", "popis": "Pojďme si pomocí symetrie jednotkové kružnice odvodit několik užitečných vzorců pro tangens.", "nazev": "Vzorce pro tangens na základě symetrie", "url": "https://cs.khanacademy.org/video/tan-symmetries-unit-circle", "id": "x2cb52cc7", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:32:34Z", "popis": "Pomocí rotace úhlů na jednotkové kružnici si odvodíme další vzorce pro sinus a kosinus.", "nazev": "Vzorce pro sinus a kosinus na základě periodicity", "url": "https://cs.khanacademy.org/video/trig-angle-rotations", "id": "x5e52b073", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:25Z", "popis": "Řešení příkladu s využitím periodicity funkce tangens.", "nazev": "Vzorce pro tangens na základě periodicity", "url": "https://cs.khanacademy.org/video/tan-periodicity", "id": "xa5a398fc", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:32:44Z", "popis": "Ukážeme si, jak pomocí jednotkové kružnice určíme hodnoty jednotlivých goniometrických funkcí z úhlu π/4.", "nazev": "Hodnoty goniometrických funkcí u speciálních úhlů", "url": "https://cs.khanacademy.org/video/solving-triangle-unit-circle", "id": "x8f71dac4", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:25Z", "popis": "Důkaz goniometrické jedničky (sinθ)^2+(cosθ)^2=1 pro všechna reálná čísla.", "nazev": "Důkaz goniometrické jedničky", "url": "https://cs.khanacademy.org/video/pythagorean-trig-identity-from-unit-circle", "id": "x952a1f00", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:32:50Z", "popis": "Máme zadáno, že sin(θ)=1/2 a že θ je ve druhém kvadrantu. Naším úkolem je za pomoci goniometrické jedničky nalézt tan(θ).", "nazev": "Použití goniometrické jedničky", "url": "https://cs.khanacademy.org/video/using-the-pythagorean-trig-identity", "id": "xda6daf69", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:26:35Z", "popis": "Proč by Tau mohlo být lepší číslo než Pí?", "nazev": "Tau versus pí", "url": "https://cs.khanacademy.org/video/tau-versus-pi", "id": "145890679", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-26T06:31:54Z", "popis": "Omluv prosím jednoduché matematické a pravopisné chyby. Neměl bys věřit ničemu, co je zde řečeno bez dvojité kontroly (i v nejlepších dobách). Klikni sem: http://tauday.com/ a zde: http://www.math.utah.edu/~palais/pi.html já: http://vihart.com", "nazev": "Pí je (stále) špatně", "url": "https://cs.khanacademy.org/video/pi-is-still-wrong", "id": "538086220", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:51Z", "popis": "Představení hlavních rysů funkcí sinus a kosinus: přímka střední hodnoty, amplituda a perioda. Ukážeme si, jak je lze nalézt z jejich grafu.", "nazev": "Vlastnosti funkcí sinus a kosinus", "url": "https://cs.khanacademy.org/video/midline-amplitude-period", "id": "x2f235e5a", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-03-07T07:30:33Z", "popis": "Najdi amplitudu a periodu funkce y=-0,5cos(3x).", "nazev": "Amplituda a perioda goniometrické funkce z jejího předpisu", "url": "https://cs.khanacademy.org/video/we-amplitude-and-period", "id": "1153684694", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:49Z", "popis": "Společně si do grafu zakreslíme funkci y=2*sin(-x). Jedná se vlastně jen o vertikální roztažení a horizontální překlopení grafu funkce y=sin(x).", "nazev": "Přetvoření grafu goniometrické funkce: vertikální roztažení a horizontální překlopení", "url": "https://cs.khanacademy.org/video/example-amplitude-and-period-transformations", "id": "1153830189", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:34Z", "popis": "Společně si do grafu zakreslíme funkci y=-2.5*cos(1/3*x). Jedná se vlastně jen o vertikální roztažení a překlopení s horizontálním roztažením grafu funkce y=cos(x).", "nazev": "Přetvoření grafu goniometrické funkce: vertikální a horizontální roztažení", "url": "https://cs.khanacademy.org/video/amplitude-and-period-cosine-transformations", "id": "1153825235", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:57Z", "popis": "Najdi předpis goniometrické funkce z jejího grafu, ve kterém je zvýrazněno minimum v bodě (-2,-5) a maximum v bodě (2,1).", "nazev": "Goniometrické funkce z grafu", "url": "https://cs.khanacademy.org/video/trig-function-equation", "id": "xdaccbc0b", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:22Z", "popis": "V této slovní úloze si pomocí goniometrické funkce namodelujeme změnu denní teploty v závislosti na čase.", "nazev": "Trigonometrické slovní úlohy: modelování denní teploty", "url": "https://cs.khanacademy.org/video/modeling-with-shifted-trig-functions", "id": "xd667e103", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:26:57Z", "popis": "Společně si povíme o funkci arkus sinus, což je inverzní funkce sinus a popíšeme její hlavní vlastnosti.", "nazev": "Úvod od arkus sinu", "url": "https://cs.khanacademy.org/video/inverse-trig-functions-arcsin", "id": "26269", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:27:05Z", "popis": "Společně si povíme o funkci arkus tangens, což je inverzní funkce sinus a popíšeme její hlavní vlastnosti.", "nazev": "Úvod do arkus tangenty", "url": "https://cs.khanacademy.org/video/inverse-trig-functions-arctan", "id": "26271", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:27:03Z", "popis": "Společně si povíme o funkci arkus kosinus, což je inverzní funkce sinus a popíšeme její hlavní vlastnosti.", "nazev": "Úvod do arkus kosinu", "url": "https://cs.khanacademy.org/video/inverse-trig-functions-arccos", "id": "26273", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-05-28T01:14:30Z", "popis": "Se zadaným grafem goniometrické funkce popiš způsoby, kterými můžeš změnit funkci na takovou, ke které lze udělat inverzní funkci.", "nazev": "Omezení definičního oboru funkcí k vytvoření inverzní funkce", "url": "https://cs.khanacademy.org/video/restricting-trig-function-domain", "id": "x5679daee", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-09-07T06:35:28Z", "popis": "Nejprve nalezneme předpis pro inverzní funkci g(x)=tan(x-3π/2)+6 a pak určíme definiční obor této inverzní funkce.", "nazev": "Definiční obor a obor hodnot inverzní funkce tangens", "url": "https://cs.khanacademy.org/video/inverse-tan-domain", "id": "x7988ecbb", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:27:41Z", "popis": "Zde se budeme zabývat vhodným způsobem použití kalkulačky k nalezení velikosti úhlu, když je uvedena jeho hodnota tangentu.", "nazev": "Inverzní goniometrické funkce za pomocí kalkulačky", "url": "https://cs.khanacademy.org/video/inverse-tan-scenario", "id": "x793351da", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:26:38Z", "popis": "Vyřešení rovnic cos(θ)=1 a cos(θ)=-1 použitím grafu funkce y=cos(θ).", "nazev": "Řešení rovnic cos(θ)=1 a cos(θ)=-1", "url": "https://cs.khanacademy.org/video/we-graph-of-cosine-function", "id": "1153795385", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:27:08Z", "popis": "V tomto videu si dáme dohromady všechny speciální případy, kterých se týkají goniometrické vzorce: sin(a+b), sin(a-c), cos(a+b), cos(a-b), cos(2a) a sin(2a).", "nazev": "Souhrn goniometrických vzorců", "url": "https://cs.khanacademy.org/video/trigonometry-identity-review-fun", "id": "26275", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:28:17Z", "popis": "V pravoúhlém trojúhelníku máme najít kosinus součtu úhlů 60° a neznámého úhlu. K tomu využijeme vzoreček.", "nazev": "Použití součtového vzorce u kosinu\n", "url": "https://cs.khanacademy.org/video/cosine-addition-identity-example", "id": "xfb68005e", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-09-09T06:35:31Z", "popis": "V pravoúhlém trojúhelníku máme najít kosinus dvojnásobného úhlu. K tomu využijeme vzoreček.", "nazev": "Použití vzorce na kosinus dvojnásobného úhlu", "url": "https://cs.khanacademy.org/video/double-angle-formula-for-cosine-example-c", "id": "x5368b1c4", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-05-23T19:15:26Z", "popis": "Dokazování vzorečku sin(x+y) = sin(x)*cos(y) + cos(x)*sin(y).", "nazev": "Důkaz součtového vzorce u sinu", "url": "https://cs.khanacademy.org/video/proof-angle-addition-sine", "id": "x8c2e47e4", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:27:40Z", "popis": "Dokazování vzorečku cos(x+y) = cos(x)*cos(y) - sin(x)*sin(y).", "nazev": "Důkaz součtového vzorce u kosinu", "url": "https://cs.khanacademy.org/video/proof-angle-addition-cosine", "id": "x9472d9c4", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:27:44Z", "popis": "Zjisti hodnotu sin(7π/12) tím, že jej přepíšeš jako sin(π/3+π/4), a pak použiješ sinový součtový vzorec.", "nazev": "Aplikace součtových vzorců", "url": "https://cs.khanacademy.org/video/sine-angle-addition-2", "id": "xd0dcedae", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:27:45Z", "popis": "Se dvěma zadanými trojúhelníky a několika délkami stran použij sinový součtový vzorec, abys našel chybějící délku strany.", "nazev": "Použití součtových vzorců: nalezení délky strany", "url": "https://cs.khanacademy.org/video/sin-angle-addition", "id": "xdf09400e", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:27:14Z", "popis": "Se dvěma zadaným cos(2θ)=C použij vzorec pro dvojnásobný úhel u kosinu k nalezení výrazu pro sin(θ).", "nazev": "Použití součtových vzorců: manipulace s výrazy", "url": "https://cs.khanacademy.org/video/cosine-angle-addition", "id": "x3448aa45", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}, {"autor": "Khan Academy", "rocnik": "3-SS", "dostupnost": "7-ANO", "licence": "1-CCBYNCSA30", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2016-10-06T19:28:04Z", "popis": "Přepiš výrazy pomocí goniometrické jedničky a dalších goniometrických vzorců.", "nazev": "Použití goniometrických vzorců", "url": "https://cs.khanacademy.org/video/examples-using-pythagorean-identities-to-simplify-trigonometric-expressions", "id": "x756fc164", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-G", "typ": "8-VI"}]