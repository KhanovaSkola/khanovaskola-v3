[{"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-10-27T06:31:14Z", "popis": "Jaký je řád číslice 3 v čísle 4 356?", "nazev": "Řád číslice", "url": "https://cs.khanacademy.org/video/place-value-1", "id": "2600657", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:49Z", "popis": "Naučíme se psát šest set čtyřicet pět milionů pět set osmdesát čtyři tisíc čtyři sta šedesát dva číslovkami.", "nazev": "Psaní čísla ve zkráceném zápisu", "url": "https://cs.khanacademy.org/video/place-value-2", "id": "2600658", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:08Z", "popis": "V tomto videu si vysvětlíme, co je to rozvinutý zápis čísla. A také si v rozvinutém zápisu zapíšeme číslo 14 897.", "nazev": "Psaní čísla v rozvinutém zápisu", "url": "https://cs.khanacademy.org/video/place-value-3", "id": "2600659", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:16Z", "popis": "Máme číslo 43 249. Jaký je rozdíl mezi dvěma 4, které v čísle máme? Jak je můžeme porovnat? A jak se můžeme pohybovat mezi číselnými řády?", "nazev": "Porovnávání řádů číslic", "url": "https://cs.khanacademy.org/video/comparing-place-values", "id": "x19b4a5d9", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:01:39Z", "popis": "Zamyslíme se, jak se nám řád číslice mění, když budeme číslo násobit 10. Řády číslic se nám při násobení 10 bude posouvat doleva a číslo se nám bude zvětšovat. Ukážeme si dva řešené příklady.", "nazev": "Pochopení řádu číslice", "url": "https://cs.khanacademy.org/video/understanding-place-value-1-exercise", "id": "x958d77f4", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:38:29Z", "popis": "Ukážeme si, jak se mění řád číslice při násobení a dělení 10. Ukážeme si dva postupy. Buď můžeme připisovat nebo škrtat nuly na konci čísla, nebo můžeme přesouvat číslice do nižších nebo vyšších řádů pomocí tabulky.", "nazev": "Řád číslice při násobení a dělení 10", "url": "https://cs.khanacademy.org/video/place-value-when-multiplying-and-dividing-by-10", "id": "x39412805", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:01Z", "popis": "Zapíšeme si číslo 4 500 jiným způsobem. Použijeme k tomu přeskupování mezi řády.", "nazev": "Přeskupení čísel jako součet rozdílných řádů", "url": "https://cs.khanacademy.org/video/regrouping-numbers-intro-various-place-values", "id": "xd5b498ba", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:13Z", "popis": "Porovnáme dvě celá čísla. Jedno je zapsané v rozvinutém zápisu, druhé slovy. Sjednotíme si je do standardního zápisu a pak je porovnáme.", "nazev": "Porovnání řádů celých čísel", "url": "https://cs.khanacademy.org/video/comparing-whole-number-place-values", "id": "x40b497d9", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:39:06Z", "popis": "Zapíšeme celá čísla pomocí různých řádů. Ukážeme si i tabulku, na které si jednoduše ukážeme, jak můžeme čísla přeskupovat mezi řády.", "nazev": "Přeskupení číselných řádů celých čísel", "url": "https://cs.khanacademy.org/video/regrouping-whole-number-place-values", "id": "xe3e20deb", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:39:06Z", "popis": "Sečteme čísla 19 tisíc + 7 desítek a 50 desetitisíc + 22 tisíc pomocí jejich řádů.", "nazev": "Sčítání celých čísel podle jejich řádů", "url": "https://cs.khanacademy.org/video/adding-whole-numbers-by-their-place-values", "id": "x929a284d", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:14Z", "popis": "Zaokrouhlíme 24 259 na stovky. Jaké pravidlo se při zaokrouhlování používá?", "nazev": "Zaokrouhlení celých čísel na stovky", "url": "https://cs.khanacademy.org/video/rounding-whole-numbers-1", "id": "2600660", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:14Z", "popis": "Zaokrouhlíme číslo 423 275 na tisíce.", "nazev": "Zaokrouhlení celých čísel na tisíce", "url": "https://cs.khanacademy.org/video/rounding-whole-numbers-2", "id": "2600661", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:14Z", "popis": "V tomto videu si zaokrouhlíme číslo 1 585 na desítky.", "nazev": "Zaokrouhlení celých čísel na desítky", "url": "https://cs.khanacademy.org/video/rounding-whole-numbers-3", "id": "2600662", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-10-27T06:31:14Z", "popis": "Přeskupíme zápis čísla 675, aby bylo napsáno různými způsoby.", "nazev": "Přeskupení celých čísel: 675", "url": "https://cs.khanacademy.org/video/regrouping-whole-numbers", "id": "x04178bbd", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-10-27T06:31:13Z", "popis": "Přeskupíme zápis čísla 76 830 podle řádů.", "nazev": "Přeskupení celých čísel: 76 830", "url": "https://cs.khanacademy.org/video/regrouping-whole-numbers-example-1", "id": "xa0f03f8e", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-10-27T06:31:17Z", "popis": "Přeskupíme zápis čísla 26 třemi způsoby.", "nazev": "Přeskupení celých čísel: 26", "url": "https://cs.khanacademy.org/video/regrouping-whole-numbers-example-2", "id": "xa60f4010", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-26T06:32:13Z", "popis": "Pořadí početních operací nám říká, v jakém pořadí máme počítat výrazy s více než jednou operací. Nejdříve vypočítáme jakoukoli operaci uvnitř závorek. Potom vypočítáme jakékoli mocnění. Následně vypočítáme všechna násobení a dělení zleva doprava. A nakonec děláme sčítání a odčítání zleva doprava.", "nazev": "Řešený příklad: pořadí početních operací s mocninami", "url": "https://cs.khanacademy.org/video/order-of-operations", "id": "2600695", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:07Z", "popis": "Na několika skupinách kuliček si ukážeme dvě důležité vlastnosti násobení. Jedna z nich je asociativita a znamená, že nezáleží na uzávorkování. Druhá je komutativita a znamená, že nezáleží na pořadí.", "nazev": "Vlastnosti násobení", "url": "https://cs.khanacademy.org/video/order-doesn-t-matter-when-purely-multiplying", "id": "x1d5c4398", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:54Z", "popis": "V tomto videu si ukážeme, že si násobení občas můžeme zjednodušit jednoduchou operací. Stačí, když si jedno z čísel rozložíme na dvě menší čísla, které se nám už budou lépe násobit.", "nazev": "Vlastnosti a vzory pro násobení", "url": "https://cs.khanacademy.org/video/properties-and-patterns-for-multiplication", "id": "x5d249382", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:01:46Z", "popis": "V tomto videu si ukážeme, co je to komutativita sčítání. Pak si při počítání vysvětlíme, k čemu nám může být tato vlastnost dobrá a kdy nám může ulehčit počítání.", "nazev": "Komutativita sčítání", "url": "https://cs.khanacademy.org/video/commutative-law-of-addition", "id": "2600686", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:02:03Z", "popis": "Vysvětlíme si, co je to komutativita násobení. Znamená to, že u násobení nezáleží na tom, v jakém pořadí budeme násobit. Jestli budeme počítat 2 krát 34 nebo 34 krát 2.", "nazev": "Komutativita násobení", "url": "https://cs.khanacademy.org/video/commutative-law-of-multiplication", "id": "2600687", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:01:44Z", "popis": "Vysvětlíme si, co je to asociativita sčítání. Spočítáme si s použitím této vlastnosti příklad (77 plus 2) plus 3.", "nazev": "Asociativita sčítání", "url": "https://cs.khanacademy.org/video/associative-law-of-addition", "id": "2600688", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:01:43Z", "popis": "Vysvětlíme si, co je to asociativita násobení a spočítáme si příklad (12 krát 3) krát 10.", "nazev": "Asociativita násobení", "url": "https://cs.khanacademy.org/video/associative-law-of-multiplication", "id": "2600689", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-11-25T07:31:23Z", "popis": "Ukážeme si, že pokud mezi sebou budeme násobit tři čísla, nebude záležet na pořadí, v jakém je budeme násobit.", "nazev": "Asociativní vlastnost násobení", "url": "https://cs.khanacademy.org/video/associate-property-of-multiplication", "id": "x34094266c8bd61ca", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:02:02Z", "popis": "Neutrální vlastnost jedničky říká, že jakékoli číslo násobené 1 zůstane původním číslem. Jinak řečeno, jakékoli číslo vynásobené 1 se nezmění. Důvodem je to, že když násobíme 1, tak vlastně máme 1 kopii našeho čísla. Například 65 ∙ 1 = 65. ", "nazev": "Neutrální vlastnost jedničky", "url": "https://cs.khanacademy.org/video/identity-property-of-1", "id": "2600685", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:03:11Z", "popis": "Ukážeme si, že jakékoli číslo plus 0 je rovno původnímu číslu.", "nazev": "Neutrální vlastnost nuly", "url": "https://cs.khanacademy.org/video/identity-property-of-0", "id": "13179315", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:03:17Z", "popis": "Ukážeme si, co je číslo opačné. Jeho myšlenka je jednoduchá. Číslo plus číslo k němu opačné se rovnají nule.", "nazev": "Existence opačného čísla ve sčítání", "url": "https://cs.khanacademy.org/video/inverse-property-of-addition", "id": "261091818", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:03:20Z", "popis": "Je to jednoduchá myšlenka, že násobení čísla s jeho převrácenými číslem dává jako výsledek vždy 1.5 ∙ 1/5 = 1", "nazev": "Existence převráceného čísla v násobení", "url": "https://cs.khanacademy.org/video/inverse-property-of-multiplication", "id": "261091817", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:19Z", "popis": "Nauč se používat distributivitu násobení a sčítání a zjisti, jak funguje. Můžeme se setkat i s pojmy distributivní zákon, nebo distributivní vlastnost.", "nazev": "Distributivita ve sčítání", "url": "https://cs.khanacademy.org/video/the-distributive-property", "id": "2600690", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:19Z", "popis": "V tomto příkladu si ukážeme, jak bude distributivita vypadat, pokud ji budeme aplikovat na odčítání. Tentokrát dva roznásobené výsledky nebudeme sčítat, ale budeme je od sebe odčítat.", "nazev": "Distributivita v odčítání", "url": "https://cs.khanacademy.org/video/the-distributive-property-2", "id": "2600691", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:00:42Z", "popis": "Když si s námi projdeš tento příklad, budeš už na distributivitu úplný odborník.", "nazev": "Distributivita - řešený příklad", "url": "https://cs.khanacademy.org/video/distributive-property-exercise", "id": "x4cf5b8e4", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:32:34Z", "popis": "Řekneme si, jaká čísla jsou racionální a jaká jsou iracionální. Jak čísla do těchto dvou skupin rozdělujeme?", "nazev": "Úvod do racionálních a iracionálních čísel", "url": "https://cs.khanacademy.org/video/introduction-to-rational-and-irrational-numbers", "id": "x2d1f5fa7", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:42Z", "popis": "Budeme určovat, která z čísel jsou racionální a která jsou iracionální.", "nazev": "Číselné obory: racionální a iracionální čísla", "url": "https://cs.khanacademy.org/video/recognizing-irrational-numbers", "id": "x00ce2e4a", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:00:44Z", "popis": "Řešené příklady pro rozpoznávání základních pravidel dělitelnosti. Jak poznám, že je nějaké číslo dělitelné jiným beze zbytku? V tomto videu si ukážeme, jak to poznáme u dělitelů 2, 3, 4, 5, 6, 9 a 10.", "nazev": "Znaky dělitelnosti přírozených čísel - 2, 3, 4, 5, 6, 9, 10", "url": "https://cs.khanacademy.org/video/divisibility-tests-for-2-3-4-5-6-9-10", "id": "1245177424", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:02:35Z", "popis": "Urči, zda je číslo 380 dělitelné beze zbytku čísly 2, 3, 4, 5, 6, 9 a 10.", "nazev": "Znaky dělitelnosti - příklad", "url": "https://cs.khanacademy.org/video/recognizing-divisibility", "id": "2600702", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:00:38Z", "popis": "Proč funguje pravidlo, že číslo je dělitelné 3, pokud je jeho ciferný součet dělitelný 3? V tomto videu si nejen ukážeme, ale i dokážeme, že to skutečně platí. ", "nazev": "Jak funguje pravidlo dělitelnosti číslem 3", "url": "https://cs.khanacademy.org/video/the-why-of-the-3-divisibility-rule", "id": "884395195", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:03:15Z", "popis": "Vysvětlíme si, jak odhalíme číslo dělitelné devíti a proč je tato dělitelnost spojená s dělitelností ciferného součtu.", "nazev": "Jak funguje pravidlo dělitelnosti číslem 9", "url": "https://cs.khanacademy.org/video/the-why-of-the-9-divisibility-rule", "id": "1245280895", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:45Z", "popis": "Najdi všechny dělitele čísla 120.", "nazev": "Hledání dělitelů čísla", "url": "https://cs.khanacademy.org/video/finding-factors-of-a-number", "id": "2600703", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-26T06:31:56Z", "popis": "Pomocí znaků dělitelnosti budeme určovat, která z uvedených čísel jsou děliteli 154 a která násobky 14.", "nazev": "Hledání dělitelů a násobků", "url": "https://cs.khanacademy.org/video/finding-factors-and-multiples", "id": "xb863d3f2", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:52Z", "popis": "Prvočíslo je takové číslo, které je dělitelné pouze jedničkou a samo sebou. Pojďme se o takových číslech dozvědět více. ", "nazev": "Prvočísla", "url": "https://cs.khanacademy.org/video/prime-numbers", "id": "85595310", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:27Z", "popis": "Dokážete určit prvočísla ve skupině čísel? Co je složené číslo? Ukážeme si, jak určíme, zda číslo je prvočíslo, složené číslo nebo ani jedno.", "nazev": "Rozlišování prvočísel a složených čísel", "url": "https://cs.khanacademy.org/video/recognizing-prime-numbers", "id": "158925083", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-11-02T06:30:50Z", "popis": "V tomto videu si řekneme, co je to prvočíselný rozklad. Pak si na prvočísla rozložíme číslo 75.", "nazev": "Prvočíselný rozklad", "url": "https://cs.khanacademy.org/video/prime-factorization", "id": "2600704", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:00:40Z", "popis": "Procvičování prvočíselného rozkladu", "nazev": "Procvičování prvočíselného rozkladu", "url": "https://cs.khanacademy.org/video/prime-factorization-exercise", "id": "xf27acc45", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-11-02T06:30:51Z", "popis": "Pokud víme, že nějaká čísla jsou dělitelná 20 a 12, jakými čísly ještě budou dělitelná? Dva příklady tohoto typu vyřešíme s pomocí rozkladu na prvočísla.", "nazev": "Procvičování dělitelnosti", "url": "https://cs.khanacademy.org/video/common-divisibility-examples", "id": "56195550", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:49Z", "popis": "Společně zjistíme nsn (nejmenší společný násobek) čísel 12 a 36, a 12 a 18. Ukážeme si, jak správně čísla provádět prvočíselný rozklad. ", "nazev": "Nejmenší společný násobek", "url": "https://cs.khanacademy.org/video/least-common-multiple-exercise", "id": "x19e7b12a", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:06Z", "popis": "Společně zjistíme nsn (nejmenší společný násobek) čísel 25 a 30. Jak budeme postupovat, pokud jsou některé číslice v prvočíselném rozkladu stejné?", "nazev": "Nejmenší společný násobek: opakované číslice", "url": "https://cs.khanacademy.org/video/least-common-multiple-exercise-2", "id": "xafe97682", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:05Z", "popis": "V tomto příkladě si ukážeme, jak zjistit nsn tří čísel: 15, 6 a 10. Je to sice obtížnější, zato celkem zábavný příklad. Pojďte si ho tedy s námi vyzkoušet!", "nazev": "Nejmenší společný násobek tří čísel", "url": "https://cs.khanacademy.org/video/least-common-multiple-lcm", "id": "158925086", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:42Z", "popis": "Najdi číslo, které je dělitelem dvou zadaných čísel. Pokud jich je víc, chceme to největší. Takovému číslu říkáme největší společný dělitel a označujeme jej jako nsd (podle počátečních písmen). Pojďme si na několika příkladech ukázat, jak na to.", "nazev": "Příklady na největší společný dělitel", "url": "https://cs.khanacademy.org/video/greatest-common-divisor-factor-exercise", "id": "x0fbf66ba", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-11-02T06:30:48Z", "popis": "Na několika ukázkových příkladech si ukážeme, jak najít největší společný dělitel dvou čísel. Pojďme na to!", "nazev": "Vysvětlení největšího společného dělitele", "url": "https://cs.khanacademy.org/video/greatest-common-divisor", "id": "23860", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:32:55Z", "popis": "Teď se pustíme do slovních úloh. Budeme kombinovat zadání pro největšího společného dělitele a nejmenší společný násobek. Zadání si nejdříve pozorně pročteme, abychom zjistili, co máme počítat a zjišťovat.", "nazev": "Slovní úlohy na největší společný dělitel a nejmenší společný násobek", "url": "https://cs.khanacademy.org/video/lcm-and-gcf-greatest-common-factor-word-problems", "id": "x59fe3ed2", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:22Z", "popis": "Představíme si procenta a vysvětlíme si, jak souvisí s čtverečkovou mřížkou 10 krát 10, ve které je 100 čtverečků. A pak si vybarvíme 20 %.", "nazev": "Co to jsou procenta?", "url": "https://cs.khanacademy.org/video/describing-the-meaning-of-percent", "id": "4802229", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:40Z", "popis": "Procenta jsou části sta. Takže, jak to funguje v případech větších než 100 %, kterým je například 109 % procent?", "nazev": "Co znamená 109 %", "url": "https://cs.khanacademy.org/video/describing-the-meaning-of-percent-2", "id": "4802230", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-06-27T16:02:53Z", "popis": "Ze zlomkových koláčů budeme vyjadřovat procenty, jaká část je vybarvená.", "nazev": "Zlomkové koláče", "url": "https://cs.khanacademy.org/video/percent-from-fraction-models", "id": "xbd10d28340636b83", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-06-27T16:02:55Z", "popis": "Ze zlomkových koláčů si vyjádříme, jaká část celku je vybarvena. Nejprve zlomek, pak desetinným číslem a nakonec procenty.", "nazev": "Znázornění zlomků, desetinných čísel a procent", "url": "https://cs.khanacademy.org/video/fraction-decimal-and-percent-from-visual-model", "id": "x722ac3f3a18ed682", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-26T06:32:02Z", "popis": "Procenta lze snadno zapsat jako desetinné číslo. Procenta vyjadřují část ze sta. Proto pro převod na desetinné číslo stačí procento vydělit číslem 100. Zároveň už tam poté nepíšeme %. Tak například kdybychom chtěli převést 65 %. Nejdříve jej vydělíme 65 : 100. Tím získáme výsledek 0,65. Vidíme, že stačí pouhé posunutí desetinné čárky o dvě místa doleva. A podobně funguje i opačně převod desetinných čísel na procenta.", "nazev": "Převod desetinných čísel na procenta: 0,601", "url": "https://cs.khanacademy.org/video/converting-decimals-to-percents-ex-1", "id": "915124662", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:53Z", "popis": "Ukážeme si postup, jak převést desetinné číslo na procenta. Budeme převádět číslo 1,501.", "nazev": "Převod desetinných čísel na procenta: 1,501", "url": "https://cs.khanacademy.org/video/converting-decimals-to-percents-ex-2", "id": "914998704", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-26T06:32:08Z", "popis": "Desetinná čísla lze zapsat pomocí procent. ​Procenta vyjadřují část ze 100. Proto pro převod stačí desetinné číslo vynásobit číslem 100. Nakonec jen přidáme symbol procent (%). Co kdybychom chtěli převést 0,8? Nejdříve bychom vypočítali součin 0,8 a 100. Vyšlo by nám 0,8=80 %. Vidíme, že násobení číslem 100 způsobí posunutí desetinné čárky o dvě místa doprava.  ", "nazev": "Převádění procent na desetinná čísla: 59,2 %", "url": "https://cs.khanacademy.org/video/converting-percents-to-decimals-ex-1", "id": "xf7f8fc80", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:19Z", "popis": "Ve videu si převedeme 113,9 % na desetinné číslo správným posunem desetinné čárky.", "nazev": "Převádění procent na desetinná čísla: 113,9 %", "url": "https://cs.khanacademy.org/video/converting-percents-to-decimals-ex-2", "id": "x8959c8e0", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:40Z", "popis": "V tomto videu si ukážeme, jak převést 18 % na desetinné číslo i na zlomek.", "nazev": "Příklad na převádění procent na desetinná čísla a zlomky", "url": "https://cs.khanacademy.org/video/representing-a-number-as-a-decimal-percent-and-fraction", "id": "4802231", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:03:31Z", "popis": "Naším úkolem bude seřadit čísla podle velikosti, která jsou vyjádřena různými způsoby - jako procenta, zlomky nebo jako desetinná čísla. Pro to, abychom je mohli porovnat, tedy nejprve všechna čísla převedeme na desetinná čísla.", "nazev": "Porovnávání číselných výrazů", "url": "https://cs.khanacademy.org/video/ordering-numeric-expressions_dup_3", "id": "547365605", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:02:27Z", "popis": "Společně si procvičíme procenta ve slovní úloze, kde budeme muset správně určit procenta, základ a část.\n150 je 25 % z jakého čísla?", "nazev": "Zjišťování procent a velikosti základu", "url": "https://cs.khanacademy.org/video/identifying-percent-amount-and-base", "id": "153770143", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:58Z", "popis": "Tato slovní úloha vám bude možná připadat matoucí, ale nebojte se. Vše si rozepíšeme pomocí celku a části.", "nazev": "Slovní úloha na procenta: Kolik procent z 80 je 100?", "url": "https://cs.khanacademy.org/video/solving-percent-problems-2", "id": "14", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-12-03T07:31:02Z", "popis": "Tento příklad je zaměřen na přepočet procent z části na základ. Pro vyřešení si vytvoříme jednoduchou algebraickou rovnici.", "nazev": "Slovní úloha na procenta: 78 je 15 % z jakého čísla?", "url": "https://cs.khanacademy.org/video/solving-percent-problems-3", "id": "15", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-02-09T20:03:53Z", "popis": "Zjisti, co znamená druhá odmocnina a jaké číslo násobené samo sebou nám dá požadované číslo. Kolik je odmocnina ze 100?", "nazev": "Řešený příklad: Druhá odmocnina", "url": "https://cs.khanacademy.org/video/understanding-square-roots", "id": "2600694", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:32:57Z", "popis": "Nyní si ukážeme, jak bez kalkulačky vypočítáme přibližnou hodnotu √45 .", "nazev": "Odhad druhé odmocniny se zaokrouhlením na setiny", "url": "https://cs.khanacademy.org/video/approximating-square-roots", "id": "125518448", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:34:08Z", "popis": "Už víme, co je druhá mocnina. Ale existuje i opačná operace ke třetí mocnině? Ano, je to třetí odmocnina. A na rozdíl od druhých odmocnin umíme najít i třetí odmocninu záporného čísla.", "nazev": "Úvod do třetí odmocniny", "url": "https://cs.khanacademy.org/video/introduction-to-cube-roots", "id": "x63d365e2", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-10-16T06:31:39Z", "popis": "Třetí odmocnina ze 3 430", "nazev": "Třetí odmocnina ze 3 430", "url": "https://cs.khanacademy.org/video/simplifying-cube-roots-1", "id": "xc78d5735", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-10-16T06:31:44Z", "popis": "Kolik je třetí odmocnina z -343?", "nazev": "Třetí odmocnina z -343", "url": "https://cs.khanacademy.org/video/simplifying-radical-expressions1", "id": "8187330", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:17Z", "popis": "Pojď se podívat, co nám vyjde, pokud vypočítáme třetí odmocninu z čísla -512.", "nazev": "Třetí odmocnina z -512", "url": "https://cs.khanacademy.org/video/finding-cube-roots", "id": "126711578", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2019-01-19T07:32:31Z", "popis": "Úvod do vědeckého zápisu čísel - pojďme se pobavit o důležitosti takového způsobu zápisu.", "nazev": "Úvod do vědeckého zápisu čísel", "url": "https://cs.khanacademy.org/video/scientific-notation-old", "id": "25785", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-10-24T06:31:21Z", "popis": "Více příkladů na vědecký zápis čísel", "nazev": "Příklady na vědecký zápis čísel", "url": "https://cs.khanacademy.org/video/scientific-notation-examples", "id": "25787", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:32:49Z", "popis": "Pojď se naučit, jak upravit výraz (9 ∙ 10^9) (-2 ∙ 10^-3).", "nazev": "Použití násobků mocnin deseti", "url": "https://cs.khanacademy.org/video/multiplying-multiples-of-powers-of-10", "id": "x36950ef2", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2017-04-04T06:33:13Z", "popis": "Teď když už víme, jak souvisí počet nul s mocninami deseti, pojďme se pustit do složitějších, ale zato zajímavějších příkladů.", "nazev": "Mocniny s 10", "url": "https://cs.khanacademy.org/video/patterns-in-zeros-exercise", "id": "x4bccbc14", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}, {"autor": "Khan Academy", "licence": "1-CCBYNCSA30", "dostupnost": "7-ANO", "jazyk": "5-cs", "gramotnost": "4-MA", "datum_vzniku": "2018-10-24T06:31:31Z", "popis": "Řády čísel - 2. příklad", "nazev": "Řády čísel - 2. příklad", "url": "https://cs.khanacademy.org/video/orders-of-magnitude-exercise-example-2", "id": "xe58a764b", "vzdelavaci_obor": "9-03", "stupen_vzdelavani": "2-Z", "typ": "8-VI"}]